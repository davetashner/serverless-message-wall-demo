# Configuration for dev environment
# Used by CI to render Crossplane templates before publishing to ConfigHub
#
# These values are substituted into infra/base/*.yaml.template files

# AWS Account ID (12 digits)
AWS_ACCOUNT_ID=205074708100

# AWS Region
AWS_REGION=us-east-1

# Resource prefix for naming (S3, Lambda, DynamoDB, etc.)
RESOURCE_PREFIX=messagewall

# Environment name
ENVIRONMENT=dev

# S3 bucket name (must be globally unique, already exists)
BUCKET_NAME=messagewall-demo-dev

# ConfigHub space name
CONFIGHUB_SPACE=messagewall-dev
